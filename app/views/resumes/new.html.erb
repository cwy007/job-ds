<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">提交简历到 - <%= @job.title %></h1>
    <%= simple_form_for [@job,@resume] do |f| %>
      <%= f.input :content, label: "内容" %>
      <%= f.input :attachment, label: "附件" %>
      <%= f.submit label: "提交", class: "btn btn-primary btn-block"%>
    <% end %>
  </div>
</div>
